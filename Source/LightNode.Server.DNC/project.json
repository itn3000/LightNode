{
  "version": "1.0.0-*",
  "buildOptions": {
    "compile": {
      "include": [
        "../LightNode.Server/**/*.cs"
      ],
      "exclude": [
        "../LightNode.Server/bin/**/*",
        "../LightNode.Server/obj/**/*",
        "../LightNode.Server/Properties/AssemblyInfo.cs"
      ]
    },
    "debugType": "portable"
  },
  "dependencies": {
    "NETStandard.Library": {
      "version":"1.6.0"
    },
    "LightNode.Core.DNC": {
      "target": "project",
      "version": "1.0.0-*"
    },
    "Newtonsoft.Json":"9.0.1",
    "Microsoft.AspNetCore.Owin": "1.0.0"
  },
  "frameworks": {
    "net451":{
      "frameworkAssemblies": {
        "System.Linq":"",
        "System.Threading.Tasks":""
      }
    },
    "netstandard1.6": {
      "dependencies": {
        "System.Threading.Tasks.Parallel": "4.0.1"
      },
      "imports": "dnxcore50",
      "buildOptions": {
        "define": [
          "DOTNETSTANDARD"
        ]
      }
    }
  }
}
